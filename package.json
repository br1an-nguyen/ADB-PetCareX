{
  "name": "petcarex",
  "version": "1.0.0",
  "description": "PetCareX - Pet Care Management System",
  "private": true,
  "scripts": {
    "install:all": "npm install --prefix src/backend && npm install --prefix src/frontend && npm install --prefix src/connect",
    "dev:backend": "npm run dev --prefix src/backend",
    "dev:frontend": "npm run dev --prefix src/frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "start:backend": "npm start --prefix src/backend",
    "start:frontend": "npm run build --prefix src/frontend"
  },
  "keywords": ["petcare", "pet", "management"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
